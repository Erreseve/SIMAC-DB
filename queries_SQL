1.  	select t.descripcion, t.folio, c.fecha
    	from trabajo t INNER JOIN cotizacion c 
	ON c.folio_trabajo = t.folio
    	where t.servicio = 'x' and c.fecha between 'd1' and 'd2';
    
2.  	select f.folio, f.monto, f.fecha 
    	from factura f
    	where f.monto <= f2 and f.monto >= f1 and f.fecha between 'd1' and 'd2';

3.  	select sum(g.total) as 'Gasto acumulado'
    	from gasto g
   	where g.fecha between 'd1' and 'd2';
    
4. 	select e.fecha_contratacion, datediff(e.fecha_contratacion, curdate()) as 'Antiguedad (d√≠as)'
	from empleado e 
	where e.nombre = 'tnombre' and e.apellido = 'tapellido';
	
5.	select e.nombre, e.apellido
	from empleado e 
	INNER JOIN (realiza r INNER JOIN (trabajo t INNER JOIN (empresa em INNER JOIN cotizacion c ON em.rfc = c.rfc_solicitante) 
		ON t.folio = c.folio_trabajo) ON r.folio_trabajo = t.folio) ON e.rfc = r.rfc_empleado;
		
6.	select e.nombre, e.apellido
	from empleado e INNER JOIN empleado e1 ON e.jefe = e1.rfc
	where e1.nombre = 'xnombre' and e1.apellido = 'xapellido';
	
7.	select c.folio, c.fecha, em.nombre
	from cotizacion c INNER JOIN empresa em ON c.rfc_solicitante = em.rfc
	where c.fecha between 'd1' and 'd2';
	
8.	select e.nombre, e.apellido
	from empleado e INNER JOIN (trabajo t INNER JOIN realiza r ON r.folio_trabajo = t.folio ) ON e.rfc = r.rfc_empleado;
	
	select t.folio, t.servicio, c.folio, c.fecha
9.	from cotizacion c INNER JOIN(trabajo t INNER JOIN (empleado e INNER JOIN realiza r ON e.rfc = r.rfc_empleado) 
		ON t.folio = r.folio_trabajo) ON c.folio_trabajo = t.folio
	where e.nombre = 'xnombre' and e.apellido = 'xapellido';

10.	select e.nombre, e.apellido, e.fecha_contratacion
	from empleado e 
	where e.activo = 'true'; 
	
11.	select e.nombre, e.apellido, e.fecha_contratacion
	from empleado e
	where e.activo = 'false';
	
12.	select dir.municipio, dir.colonia, dir.calle, dir.numero, dir.telefono
	from direccion_empleado dir INNER JOIN empleado e 
	ON e.rfc = dir.rfc_empleado
	where e.nombre = 'xnombre' and e.apellido = 'xapellido';
	
13.	select con.num_celular, con.correo_electronico
	from info_contacto_empleado con INNER JOIN empleado e 
	ON e.rfc = con.rfc_empleado
	where e.nombre = 'xnombre' and e.apellido = 'xapellido';
	
14.	select g.fecha, g.mano_obra, g.luz, g.gasolina, g.material, g.total
	from gasto g INNER JOIN empleado e 
	ON g.registrado_por = e.rfc
	where e.nombre = 'xnombre' and e.apellido = 'xapellido';
	
15.	select f.folio, f.fecha, f.monto
	from factura f INNER JOIN empresa em 
	ON f.rfc_empresa = em.rfc
	where em.nombre = 'xnombre';
	
16.	select f.folio, f.fecha, f.monto
	from factura f INNER JOIN trabajo t
	ON f.folio_trabajo = xfolio;
	
17.	select f.folio, f.monto
	from factura f 
	where f.fecha = xfecha;
	
18.	select *
	from cotizacion c
	where c.folio = xfolio;
	
19.	select * 
	from trabajo t
	where t.folio = xfolio;
	
20.	select *
	from factura f
	where f.folio = xfolio;
	
21.	select * from factura;

22.	select * from empleado;

23.	select * from trabajo;

24.	select * from cotizacion;

25.	select * from empresas;

26.	select * from gastos;
